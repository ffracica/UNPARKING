module ledparqueadero(clk, led, pasignado, l);

input clk;
input led;
input [3:0]pasignado;
output reg [5:0]l;


always@(posedge clk)begin


if(led)begin
	case(pasignado)
		4'b0000: l = 6'b111111;
		4'b0001: l = 6'b111110;
		4'b0010: l = 6'b111101;
		4'b0011: l = 6'b111011;
		4'b0100: l = 6'b110111;
		4'b0101: l = 6'b101111;
		4'b0110: l = 6'b011111;
		default
			l = 6'b111111;
	endcase
end
else
	l = 6'b111111;




end


endmodule 